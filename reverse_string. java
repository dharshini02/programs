class reverse_string{
    public static void main(String[] args) throws Exception{
           String str="hello";
           String str1=" ";
          char arr[]=str.atcharArray();
           int n=arr.length();
           for(int i=n;i>0;i--)
           {
               str1=str1+arr[i];
           }
        System.out.println(str1);
    }

}
